{
  "name": "adonis-starter",
  "version": "1.1.0",
  "description": "Starter pack for Adonis v.4",
  "main": "index.js",
  "scripts": {
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then node ace migration:run --force && npm run build:prod ; fi",
    "build:prod": "NODE_ENV=production mkdir -p public/build && rm -rf public/build/* && webpack -p --config webpack.prod.js",
    "build:dev": "mkdir -p public/build && rm -rf public/build/* && webpack -p --config webpack.dev.js",
    "watch": "mkdir -p public/build && rm -rf public/build/* && webpack -w --config webpack.dev.js",
    "start": "node server.js",
    "test": "node ace test"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.0",
    "@adonisjs/ally": "^2.0.2",
    "@adonisjs/auth": "^3.0.1",
    "@adonisjs/bodyparser": "^2.0.2",
    "@adonisjs/cors": "^1.0.5",
    "@adonisjs/fold": "^4.0.7",
    "@adonisjs/framework": "^5.0.4",
    "@adonisjs/ignitor": "^2.0.5",
    "@adonisjs/lucid": "^5.0.2",
    "@adonisjs/mail": "^3.0.5",
    "@adonisjs/session": "^1.0.20",
    "@adonisjs/shield": "^1.0.6",
    "@adonisjs/validator": "^5.0.3",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "less": "^3.0.0",
    "less-loader": "^4.0.5",
    "pg": "^7.4.1",
    "style-loader": "^0.20.1",
    "uglify-js": "^3.3.10",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0"
  },
  "engines": {
    "node": "9.x.x",
    "npm": "5.x.x",
    "yarn": "1.x.x"
  }
}
